<div>
    <h1>Product List</h1>
    {{#if products.length}}
        <table>
            <tr>
                <th>Title</th>
                <th>Price</th>
            </tr>
            {{#each products}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>${{this.price}}</td>
                </tr>
            {{/each}}
        </table>
    {{/if}}
</div>